	white-space: normal;
	padding: 3px 0;
}

/* style common to both labels and values */
.field.inline > * { /* display: inline-block; */
	vertical-align: top;
	position: relative;
	white-space: normal;
	overflow: hidden;
	margin: 0;
}

/* labels */
.fieldGrid {
	display: table;
}

.fieldGrid .field {
	display: table-row;
}

